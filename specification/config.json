{
  "name": "light-common-mock-api",
  "version": "1.00",
  "groupId": "com.mservicetech.api",
  "artifactId": "light-common-mock-api",
  "rootPackage": "com.mservicetech.mock.api",
  "handlerPackage":"com.mservicetech.mock.api.handler",
  "modelPackage":"com.mservicetech.mock.api.model",
  "overwriteHandler": false,
  "overwriteHandlerTest": false,
  "overwriteModel": false,
  "httpPort": 8080,
  "enableHttp": true,
  "httpsPort": 8443,
  "enableHttps": true,
  "enableRegistry": false,
  "specChangeCodeReGenOnly": true,
  "generateValuesYml": true,
  "skipPomFile": true,
  "supportDb": false,
  "generateValuesYml": true,
  "generateEnvVars": {
    "generate": true,
    "skipArray": true,
    "skipMap": true,
    "exclude": [
      "handler.yml",
      "values.yml"
    ]
  }
}
